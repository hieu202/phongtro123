package hieu.com.models;



import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name = "category")
public class Category extends BaseEntity{
	@Column(name = "code", length = 120, nullable = true)
	private String code;
	@Column(name = "value", length = 120, nullable = true)
	private String calue;
	@Column(name = "subtitle", length = 120, nullable = true)
	private String subtitle;
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getCalue() {
		return calue;
	}
	public void setCalue(String calue) {
		this.calue = calue;
	}
	public String getSubtitle() {
		return subtitle;
	}
	public void setSubtitle(String subtitle) {
		this.subtitle = subtitle;
	}
	

}
